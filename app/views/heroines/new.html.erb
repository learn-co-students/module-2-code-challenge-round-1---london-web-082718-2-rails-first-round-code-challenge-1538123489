<h1>CREATE NEW HEROINE</h1>

<% if flash[:errors] %>
  <ul>
    <% flash[:errors].each do |error| %>
      <li><%= error %></li>
    <% end %>
  </ul>
<% end %>

<%= form_for @heroine do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :super_name %>
  <%= f.text_field :super_name %>
  <%= f.collection_select :power_id, @powers, :id, :name %>
  <%= f.submit %>
<% end %>
